#define ASSIGN 1
#define IDENTIFIER 2
#define INTEGER 3
#define DEFAULT "none"
#define END 4
#define DOUBLE 5
#define VR_SZ 30
#define FN_SZ 30	//File Name lenght
#define VN_SZ 500	//Variable Name Array size
#define VV_SZ 500	//Variable values array size
#define STRING 1000 	//Array storing name and values to be used by tokenize() function
#define NOT_SET 0
#define RANDOMIZE 1
#define USE_VALUES 2
#include<stdio.h>
#include<time.h>


// Function to generate random values of type float
double d_rand()
{
	char value[30];
    int f_rv  =  random()%20 - 10;                // First Random Value
    srand(time(NULL));				//Seeding Random number generator
    int s_rv  =  random()%20 - 10;                // Second Random Value
    srand(time(NULL));				//Seeding Random number generator
    double val = (double)s_rv + (f_rv/100.0);
    return val;

}

